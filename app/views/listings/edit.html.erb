<%= render 'flash' %>


<%= form_for @listing do |f| %>
<%= f.label :title %>
<%= f.text_field :title %> <br>

<%= f.label :content %>
<%= f.text_field :content %><br>

<%= f.label:date_added %>
<%= f.date_field :date_added %><br>

<%= f.label:adoption_center_id %>
<%= f.collection_select :adoption_center_id, @adoption_centers, :id, :location %><br>

<%= f.submit %>

<% end %>

<%= button_to "Delete Listing", listing_path(@listing), method: :delete%>