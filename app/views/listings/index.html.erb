<h1>All Listings</h1>
<% if flash[:success] %>
  <h3><%= flash[:success] %></h3>
<%end%>

  <div class="flex-container">
    <% @listings.each do |listing| %>
      <div class="card"><%= listing.cat.name %><a href="<%=listing_path(listing)%>"><img src="<%= listing.cat.image_url%>">
      </div>
    <% end %>
  </div>

