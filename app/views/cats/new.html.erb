<h1>Create a New Cat!</h1>

<%= render 'listings/flash' %>

<%= form_for @cat do |f| %>
<%= f.label :name %>
<%= f.text_field :name%><br>

<%= f.label :breed %>
<%= f.text_field :breed%><br>

<%= f.label :age %>
<%= f.number_field :age%><br>

<%= f.label :image_url %>
<%= f.text_field :image_url%><br>

<%= f.label :favorite_food %>
<%= f.text_field :favorite_food %><br>

<%= f.submit%>

<%end%>